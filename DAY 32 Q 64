//Find the digit that occurs the most times in an integer number.
Input 1:
112233
Output 1:
1
Input 2:
887799
Output 2:
7
#include <stdio.h>

int main()
            {
    int num, digit, count[10] = {0}, max = 0, mostFreq = 0;

    printf("Name - SUDHANSHU SHEKHAR \nSAP ID - 590021525 \nCourse - BCA \nBatch - B5");
    printf("\n----------------------------------\n");

    printf("Enter an integer: ");
    scanf("%d", &num);

    // Count frequency of each digit
    while(num > 0)
    {
        digit = num % 10;
        count[digit]++;
        num = num / 10;
    }

    // Find the digit with maximum frequency
    for(int i = 0; i < 10; i++)
    {
        if(count[i] > max)
        {
            max = count[i];
            mostFreq = i;
        }
    }

    printf("Digit that occurs the most times = %d\n", mostFreq);

    return 0;
}
