//Write a program to print the product of even numbers from 1 to n.
#include <stdio.h>
int main()
      {
    int n, i;
    long long product = 1;  

    printf("Name - SUDHANSHU SHEKHAR \nSAP ID - 590021525 \nCourse - BCA \nBatch - B5");
    printf("\n----------------------------------\n");

    printf("Enter the value of n: ");
    scanf("%d", &n);

    for(i = 1; i <= n; i++)
    {
        if(i % 2 == 0)
        {
            product = product * i;
        }
    }

    printf("Product of even numbers from 1 to %d = %lld", n, product);

    return 0;
}
