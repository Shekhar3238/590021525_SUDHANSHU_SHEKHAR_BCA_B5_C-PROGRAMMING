//Write a program to print the following pattern:
    5
   45
  345
 2345
12345

#include <stdio.h>
int main()
            {
    int i, j, space;

    printf("Name - SUDHANSHU SHEKHAR \nSAP ID - 590021525 \nCourse - BCA \nBatch - B5");
    printf("\n----------------------------------\n");

    for(i = 5; i >= 1; i--)
    {
        for(space = 1; space < i; space++)
        {
            printf(" ");
        }
        for(j = i; j <= 5; j++)
        {
            printf("%d", j);
        }
        printf("\n");
    }

    return 0;
}
