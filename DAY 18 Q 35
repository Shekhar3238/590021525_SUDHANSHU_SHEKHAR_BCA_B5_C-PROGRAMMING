//Write a program to print all factors of a given number.
#include <stdio.h>
int main()
      {
    int num, i;

    printf("Name - SUDHANSHU SHEKHAR \nSAP ID - 590021525 \nCourse - BCA \nBatch - B5");
    printf("\n----------------------------------\n");

    printf("Enter a number: ");
    scanf("%d", &num);

    printf("Factors of %d are: ", num);
    for(i = 1; i <= num; i++)
    {
        if(num % i == 0)
            printf("%d ", i);
    }

    return 0;
}
