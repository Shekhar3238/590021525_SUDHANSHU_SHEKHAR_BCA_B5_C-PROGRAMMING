//Write a program to swap two numbers using a third variable.
#include <stdio.h>
  int main()
    {
    int a, b, temp;
	printf("Name - SUDHANSHU SHEKHAR \nSAP ID - 590021525 \nCourse - BCA \nBatch-B5");
	printf("\n----------------------------------\n");
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    temp = a;
    a = b;
    b = temp;
    printf("After swapping: a = %d, b = %d", a, b);
    return 0;
}
